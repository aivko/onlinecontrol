<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence" version="2.1">

    <persistence-unit name="jpa.unit">
        <provider>org.datanucleus.api.jpa.PersistenceProviderImpl</provider>
        <class>com.vizaco.onlinecontrol.model.Parent</class>
        <class>com.vizaco.onlinecontrol.model.Student</class>
        <exclude-unlisted-classes>true</exclude-unlisted-classes>
        <properties>
            <property name="datanucleus.ConnectionURL" value="appengine" />
            <property name="datanucleus.appengine.ignorableMetaDataBehavior" value="NONE" />
        </properties>
    </persistence-unit>

</persistence>
