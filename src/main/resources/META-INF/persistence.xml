<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence" version="2.1">

    <!--<persistence-unit name="jpa.unit">-->
    <persistence-unit name="default">
        <provider>org.datanucleus.api.jpa.PersistenceProviderImpl</provider>
        <class>com.vizaco.onlinecontrol.model.BaseEntity</class>
        <class>com.vizaco.onlinecontrol.model.Person</class>
        <class>com.vizaco.onlinecontrol.model.Parent</class>
        <class>com.vizaco.onlinecontrol.model.Student</class>
        <class>com.vizaco.onlinecontrol.model.Teacher</class>
        <class>com.vizaco.onlinecontrol.model.User</class>
        <class>com.vizaco.onlinecontrol.model.Role</class>
        <class>com.vizaco.onlinecontrol.model.Subject</class>
        <class>com.vizaco.onlinecontrol.model.School</class>
        <class>com.vizaco.onlinecontrol.model.News</class>
        <class>com.vizaco.onlinecontrol.model.Shedule</class>
        <class>com.vizaco.onlinecontrol.model.Grade</class>
        <class>com.vizaco.onlinecontrol.model.Clazz</class>
        <exclude-unlisted-classes>true</exclude-unlisted-classes>
        <properties>
            <property name="datanucleus.ConnectionURL" value="appengine" />
            <property name="datanucleus.appengine.ignorableMetaDataBehavior" value="NONE" />
            <property name="format_sql" value="true"/>
        </properties>

    </persistence-unit>
    

</persistence>
